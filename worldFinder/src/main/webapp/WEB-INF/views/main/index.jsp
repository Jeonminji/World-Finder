    <%--이미지 맵에 하이라이트 걸어줌--%>
    <script type="text/javascript" src="http://davidlynch.org/projects/maphilight/jquery.maphilight.min.js"></script>
    <script>
        $(function() {
            $('#worldMap area').on('click', function () {
                continentView($(this).attr('alt'))

            });
            $(".continentImg area").on('click',function () {
                smallContinentView($(this).attr('alt'))
            })
        });
    </script>
    <script>
        $.fn.maphilight.defaults = {
            fill: true,  //이미지맵 링크에 마우스가 올라오면 색을 넣을 건지 여부
            fillColor: 'a3d1eb',  // 색상지정
            fillOpacity: 0.5,  // 투명도 지정 0~1
            stroke: true,    // border를 넣을건지 여부 false로 설정하면 이하 옵션 무시됨
            strokeColor: 'ffffff',  //border 색상
            strokeOpacity: 1,  //border 투명도
            strokeWidth: 1,  //border 폭
        }
        // 만약 이미지 맵이 숨김처리 되있으면 인식을 못함
        $('img[usemap]').maphilight();
    </script>
    <script>
        // 대륙 클릭시
        function continentView(a) {
            let continent;
            switch (a){
                case "북아메리카" : continent = $("#northAmericaSpace");
                    break;
                case "남아메리카" : continent = $("#southAmericaSpace");
                    break;
                case "아시아" : continent = $("#asiaSpace");
                    break;
                case "유럽" : continent = $("#europeSpace");
                    break;
                case "오세아니아" : continent = $("#oceaniaSpace");
                    break;
                case "아프리카" : continent = $("#africaSpace");
                    break;
                case  "중앙아메리카" : location.href = "/countrySearch/"+ a; return false;
                    break;
            }
            $("#backBtn").show();
            $("#worldMapView").hide();
            continent.show();
            continent.attr("name", "now");;
            // 이것도 hide hidden 처리는 인식을 못해서 할때마다 선언
            // $('img[usemap]').rwdImageMaps();
        }

        // 세부대륙 클릭시 근데 하위 나라정보가 있을때
        function smallContinentView(a) {
            let sum;
            switch (a){
                case "동아시아" : sum = $("#eastAsiaSpace");
                    break;
                case "중앙아시아" : sum = $("#centralAsiaSpace");
                    break;
                case  "동남아시아" : sum = $("#southeastAsiaSpace");
                    break;
                case "남아시아" : sum = $("#southAsiaSpace");
                    break;
                case  "서아시아" : sum = $("#westAsiaSpace");
                    break;
                default : return false;
            }
            $(".continentImg").hide();
            sum.show();
            // $('img[usemap]').rwdImageMaps();
        }

        // 뒤로가기 버튼 클릭시
        $("#backBtn").on('click', function () {
                //display none 이면 false 반환
            if ($(".continentImg").is(':visible')){
                $(".continentImg").hide();
                $("#backBtn").hide();
                $("#worldMapView").show();
                $("[name='now']").removeAttr("name");
            }
            if ($(".smallContinentImg").is(':visible')){
                $("[name='now']").show();
                $(".smallContinentImg").hide();
            }
            // $('img[usemap]').rwdImageMaps();
        })
    </script>
    <script>
        $(".continentImg").hide();
        $(".smallContinentImg").hide();
        $(".continentImg").css("visibility","visible")
        $(".smallContinentImg").css("visibility","visible")


        // 나라 이미지 클릭시 이동
        $(".countrySearch").on("click",function () {
            location.href = "/country/" + $(this).attr('alt');
        })
        // 세부대륙 클릭시 이동
        $(".continentSearch").on("click",function () {
            location.href = "/countrySearch/"+ $(this).attr('alt');
        })
    </script>
